                          <div x-text="weatherData.current.condition.text" class="text-xs text-gray-600 dark:text-gray-400"></div>
                        </div>
                        <div class="w-24">
                          <img :src="`https:${weatherData.current.condition.icon}`" :alt="weatherData.current.condition.text" loading="lazy">
                        </div>
                      </div>
                
